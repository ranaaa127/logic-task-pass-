scanf("%d%d,&num1,&num2);
if(num2>2)
{
printf("there are no primes upto %d/n",num2);
exit(0);
}
printf("prime numbers are/n");
temp=num1;
}
flag=0;
for(j=2;j<=i/2;j++)
{
flag=1;
break;
}
}
if (flag==0)
{ printf("%d/n",i);
count++;
}
}
printf("number of primes between %d &%d/n" , temp,num2,count);



